<!DOCTYPE html>
<html lang="fr-FR">
    <head>
        <meta charset="UTF-8">
        <title>Erdiwo - {% block title %}{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('assets/index.css') }}">
        <script type="text/javascript" src="{{ asset('assets/chartjs/chart.umd.js') }}"></script>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_link_tags('css/fontawesome') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}

        {% block stylesheet %}{% endblock %}
        {% block javascript %}{% endblock %}
    </head>
    <body>
        <div class="container"> {{ include('_partials/_header.html.twig') }} </div>
        <main role="main"> {% block body %}{% endblock %} </main>
    </body>
</html>
